<!DOCTYPE html>
<html>

<%- include('./partials/head') %> 

<body>
  <%- include('./partials/header') %> 
  <div class="container-total">
  <!-- categorias carteles -->
  .
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Letreros</h2>
			</div>
			<!-- Show the "visited" products -->
      <div class="container-carteles">
			<% Letreros.forEach(pr => { %>

      
        <div class="col-12 col-sm-6 col-lg-3">
					<section class="product-box">
						<a href="/products/detail/<%= pr.id %>">
							<figure class="product-box_image">
								<img src="/images/<%= pr.images %>" alt="imagen de producto">
							</figure>
							<article class="product-box_data">
								<h2><%= pr.title %></h2>
								<span>descuento % OFF</span>
								<p><%= pr.extra %></p>
								<i class="fas fa-truck"></i>
							</article>
						</a>
					</section>
				</div>			
			<%}); %>
			</div>

		</div>
	</div>

  <!-- categoria banners -->
      <div class="container products-wrapper">
        <div class="row">
          <div class="col-12">
            <h2 class="products-title">Carteles</h2>
          </div>
          <!-- Show the "in-sale" products -->
          <div class="container-stickers">
                <% Carteles.forEach(pr => { %>
                    <div class="container-box">
                      <div class="col-12 col-sm-6 col-lg-3">
                        <section class="product-box">
                          <a href="/products/detail/<%= pr.id %>">
                            <figure class="product-box_image">
                              <img src="/images/<%= pr.images %>" alt="imagen de producto">
                            </figure>
                            <article class="product-box_data">
                              <h2><%= pr.title %></h2>
                              <span>descuento % OFF</span>
                              <p><%= pr.extra %></p>
                              <i class="fas fa-truck"></i>
                            </article>
                          </a>
                        </section>
                      </div>
                    </div>
                  
                <%}); %>
            </div>
        </div>
      </div>


			

